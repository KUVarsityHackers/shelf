{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["static/", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "shelf docs",
        "disableSort": false,
        "collapse": true
    },
    "opts": {
        "destination": "static/docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./docs/templates/jsdoc-template"
    }
}